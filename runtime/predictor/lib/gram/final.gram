#JSGF V1.0;

/**
 * JSGF Digits Grammar for Hello World example
 */

grammar digits;

public <statement1> = <question> <abstract>;
public <question> = what is;
public <abstract> = (love|death|life|hell|heaven);
public <statement2> = i am a <role>;
public <self> = i am a;
public <role> = (student|teacher|lover|husband|wife|son|daughter);
