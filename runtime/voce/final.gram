#JSGF V1.0;

/**
 * JSGF Digits Grammar for Hello World example
 */

grammar digits;

public <numbers> = (oh | zero | one | two | three | four | five | six | seven | eight | nine) * ;
public <tevin> = ( can | we | talk | for | a | minute | girl | i | want | to | know | your | name) * ;
public <journey> = (i | wanna | know | what | love | is | want | you | to | show | me ) * ;
public <killers> = (i'm | coming | out | of | my | cage | and | i've | been | doing | just | fine ) * ;
